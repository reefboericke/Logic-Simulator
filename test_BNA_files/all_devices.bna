begin devices:
    CLOCK clock: period = 1;
    SWITCH switch: initial = 1;
    AND and: inputs = 1;
    NAND nand: inputs = 1;
    OR or: inputs = 1;
    NOR nor: inputs = 1;
    DTYPE dtype;
    XOR xor;
end devices;

begin connections:
    clock -> and.I1;
    switch -> nand.I1;
    and -> or.I1;
    nand -> nor.I1;
    switch -> dtype.DATA;
    clock -> dtype.CLK;
    or -> dtype.SET;
    nor -> dtype.CLEAR;
    dtype.Q -> xor.I1;
    dtype.QBAR -> xor.I2;
end connections;

begin monitors:
    clock;
    switch;
    and;
    nand;
    or;
    nor;
    dtype;
    xor;
end monitors;
    
    

    
